
<!-- JS -->
<script>
	/* Reset img's src tag to stop loading */
	var $imgs = document.getElementsByTagName( 'img' );
	var $img, src;
	
	for ( var i = 0; i < $imgs.length; i++ ) {
		$img		= $imgs[i];
		src			= $img.src;
		
		$img.src	= '{{ Path.URL.img }}bgs/pattern-transparent.png';
		$img.setAttribute( 'data-src' , src );
	}
	
	
	/* JS vars */
	var STF_Config = JSON.parse( '{{ Config|json_encode|e("js") }}' );
</script>


