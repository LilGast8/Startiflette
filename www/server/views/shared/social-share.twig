
	<!-- Social share -->
	
	<!-- Facebook -->
	<meta property="og:title" content="{{ metas.fb.title }}" />
	<meta property="og:site_name" content="{{ metas.fb.title }}" />
	<meta property="og:description" content="{{ metas.fb.desc }}" />
	<meta property="og:url" content="{{ Path.URL.base ~ Lang.LANG_LINK_ROOT }}" />
	<meta property="og:image" content="{{ Path.URL.img }}others/share.jpg" />
	<meta property="og:type" content="{{ metas.fbType }}" />
	<!-- Google Plus -->
	<meta itemprop="name" content="{{ metas.gp.title }}" />
	<meta itemprop="description" content="{{ metas.gp.desc }}" />
	<meta itemprop="image" content="{{ Path.URL.img }}others/share.jpg" />
	<!-- Twitter -->
	<meta name="twitter:card" content="summary_large_image" />
	<meta name="twitter:title" content="{{ metas.tw.title }}" />
	<meta name="twitter:description" content="{{ metas.tw.desc }}" />
	<meta name="twitter:site" content="{{ metas.twSiteId }}" />
	<meta name="twitter:creator" content="{{ metas.twCreatorId }}" />
	<meta name="twitter:url" content="{{ Path.URL.base ~ Lang.LANG_LINK_ROOT }}" />
	<meta name="twitter:image" content="{{ Path.URL.img }}others/share.jpg" />
